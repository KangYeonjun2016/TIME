<core:FragmentDefinition xmlns="sap.m"
    xmlns:custom="sap.ui.time.control"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core">
    <layout:FixFlex id='chartFixFlex' minFlexSize="250" visible="{/VizFrameVisible}">
        <layout:flexContent>
            <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}" height='100%' width="100%" vizType='line' legendVisible="false">
                <viz:dataset>
                    <viz.data:FlattenedDataset data="{/vizFrameList}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition name="Month" value="{Month}" />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition name="건 수" value="{Count}" />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>
                <viz:feeds>
                    <viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure" values="건 수" />
                    <viz.feeds:FeedItem id='categoryAxisFeed' uid="categoryAxis" type="Dimension" values="Month" />
                </viz:feeds>
                <!-- <viz:dependents>
                    <viz:Popover id="idPopOver"></viz:Popover>
                </viz:dependents> -->
            </viz:VizFrame>
        </layout:flexContent>
    </layout:FixFlex>
</core:FragmentDefinition>