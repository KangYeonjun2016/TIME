    xmlns:custom="sap.ui.time.control"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core">
    <FlexBox alignItems="Start">
        <custom:VBox width="120%" press=".onClickForTable1" class="timeReportSummaryBoxLabel">
            <Text text="근무계획 미입력"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick1" class="myVBox timeReportSummaryBoxData">
            <Text text="5" class="myVBox"/>
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable1" class="timeReportSummaryBoxLabel">
            <Text text="실적 미확정"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick2" class="myVBox timeReportSummaryBoxData">
            <Text text="10" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable1" class="timeReportSummaryBoxLabel">
            <Text text="마감 미진행"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick3" class="myVBox timeReportSummaryBoxData">
            <Text text="5" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable2" class="timeReportSummaryBoxLabel">
            <Text text="이석 발생"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick4" class="myVBox timeReportSummaryBoxData">
            <Text text="10" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable2" class="timeReportSummaryBoxLabel">
            <Text text="지각 발생"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick5" class="myVBox timeReportSummaryBoxData">
            <Text text="8" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable2" class="timeReportSummaryBoxLabel">
            <Text text="조퇴 발생"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick6" class="myVBox timeReportSummaryBoxData">
            <Text text="10" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable3" class="timeReportSummaryBoxLabel">
            <Text text="근무초과 위험"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick7" class="myVBox timeReportSummaryBoxData">
            <Text text="3" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable3" class="timeReportSummaryBoxLabel">
            <Text text="근무 미달"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick8" class="myVBox timeReportSummaryBoxData">
            <Text text="5" />
        </custom:VBox>
        <custom:VBox width="120%" press=".onClickForTable3" class="timeReportSummaryBoxLabel">
            <Text text="근무 추가"/>
        </custom:VBox>
        <custom:VBox width="80%" press=".onClick9" class="myVBox timeReportSummaryBoxDataEnd">
            <Text text="8" />
        </custom:VBox>
    </FlexBox>
    <VBox class="pt-10-px">
        <Text text="* 근무 미달 : 1일 8시간 근무 대비 20% 이상 미달한 건수입니다." />
        <Text text="* 근무 추가 : 1일 8시간 근무 대비 20% 이상 추가한 건수입니다." />
    </VBox>
